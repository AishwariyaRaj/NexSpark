# Chatbot Service Configuration
PORT=8086

# AI Provider Configuration
# Choose one: 'openai', 'gemini', or 'local'
AI_PROVIDER=openai

# OpenAI Configuration (if using OpenAI)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Google Gemini Configuration (if using Gemini)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro

# Local Model Configuration (if using local model)
LOCAL_MODEL_URL=http://localhost:11434/api/generate
LOCAL_MODEL_NAME=llama2

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Service URLs (for context gathering)
AVAILABILITY_SERVICE_URL=http://localhost:8082
BOOKING_SERVICE_URL=http://localhost:8081
